<root>
	<Implements>

	</Implements>

	<Properties>

	</Properties>

	<BaseMethods>
       <!-- 客户端请求制作装备-->
        <onClientMakeEquip>
            <Exposed/>
            <Arg>UINT32</Arg>
        </onClientMakeEquip>

        <!--客户端请求背包装备星级-->
        <onClientGetEquipStar>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
        </onClientGetEquipStar>

         <!--返回球员装备列表-->
        <onClientGetEquipList>
            <Exposed/>
            <Arg>UINT64</Arg>
        </onClientGetEquipList>

         <!--获得背包装备列表-->
        <onClientGetBagEquipList>
            <Exposed/>
        </onClientGetBagEquipList>

         <!--客户端请求背包装备升星-->
        <onClientEquipUpStar>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
        </onClientEquipUpStar>

         <!--客户端请求装备升级-->
        <onClientEquipUpStrongLv>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
        </onClientEquipUpStrongLv>

         !--客户端请求装备一键升级-->
        <onClientEquipOneKeyUPStrong>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
        </onClientEquipOneKeyUPStrong>

         !--客户端请求穿戴装备-->
        <onClientPutOnEquip>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
        </onClientPutOnEquip>

       !--客户端请求脱掉装备-->
        <onClientTakeOffEquip>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT32</Arg>
        </onClientTakeOffEquip>

        !--客户端请求镶嵌宝石-->
        <onClientInsetGem>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
            <Arg>UINT32</Arg>
        </onClientInsetGem>

       !--客户端请求卸下宝石-->
        <onClentTakeOffGem>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
        </onClentTakeOffGem>

       !--客户端请求宝石开槽-->
        <onClientGemOpen>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
        </onClientGemOpen>

        !--客户端请求装备传承-->
        <onClientEqupInherit>
          <Exposed/>
            <Arg>UINT32</Arg>
            <Arg>UINT64</Arg>
            <Arg>UINT64</Arg>
        </onClientEqupInherit>

      !--客户端请求宝石合成-->
        <onClientGemCompound>
          <Exposed/>
            <Arg>UINT64</Arg>
        </onClientGemCompound>

	</BaseMethods>

	<CellMethods>

	</CellMethods>

	<ClientMethods>
       <!-- 通知客户端制作装备成功-->
        <makeEquipSucc>
          <Arg>UINT32</Arg>
        </makeEquipSucc>


        <!-- 通知客户端宝石合成成功-->
        <gemCompoundSucc>
          <Arg>UINT32</Arg>
        </gemCompoundSucc>

         <!--返回装备列表-->
        <getEquipList>
           <Arg>UINT32</Arg>
           <Arg>  ARRAY <of> Equip_Info </of> </Arg>
        </getEquipList>

       <!--返回宝石镶嵌开孔结果-->
        <returnGemResult>
           <Arg>UINT32</Arg>
            <Arg>  ARRAY <of> Equip_Info </of> </Arg>
        </returnGemResult>

        <!--返回装备传承结果-->
        <returnInheritResult>
           <Arg>UINT32</Arg>
            <Arg>  ARRAY <of> Equip_Info </of> </Arg>
        </returnInheritResult>

        <!-- 通知客户端装备升星成功-->
        <starUpEquipSucc>
          <Arg>UINT32</Arg>
          <Arg>Equip_Info</Arg>
        </starUpEquipSucc>

        <!-- 通知客户端装备强化成功-->
        <strongUpEquipSucc>
          <Arg>UINT32</Arg>
          <Arg>Equip_Info</Arg>
        </strongUpEquipSucc>

         <!-- 通知客户端装备星级和强化等级-->
         <getStarAndStrongLv>
             <Arg>UINT32</Arg>
             <Arg>UINT32</Arg>
        </getStarAndStrongLv>

         <!--返回装备一键强化结果列表-->
        <getOneKeyUpStrongResult>
           <Arg>UINT32</Arg>
           <Arg>Equip_Info</Arg>
           <Arg>  ARRAY <of> OneKeyStrongUp_Info </of> </Arg>
        </getOneKeyUpStrongResult>

         <!-- 通知客户端穿戴装备成功-->
         <putOnEquipSucc>
             <Arg>UINT32</Arg>
        </putOnEquipSucc>
       <!-- 通知客户端穿戴脱掉成功-->
         <takeOffEquipSucc>
             <Arg>UINT32</Arg>
        </takeOffEquipSucc>

        <!--返回装备错误信息-->
        <onEquipError>
            <Exposed/>
            <Arg>UINT32</Arg>
        </onEquipError>

        <onUpdateCardInfo>
            <Arg> Card_Info </Arg>
        </onUpdateCardInfo>
	</ClientMethods>
</root>

