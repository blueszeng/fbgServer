<root>
    <Properties>

        <roleID>
            <Type>DBID</Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </roleID>

        <!-- 好感度 -->
        <liking>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </liking>

        <isFullTime>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </isFullTime>

        <periodTime>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </periodTime>

        <likingTime>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </likingTime>

        <bodyIndex>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </bodyIndex>

        <headIndex>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </headIndex>

        <chestIndex>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </chestIndex>

        <legIndex>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </legIndex>

        <handIndex>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </handIndex>

        <!-- 好感度变满的时间戳 -->
        <fullTime>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </fullTime>


        <!-- 亲密抚摸次数 -->
        <closeTouch>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </closeTouch>

        <!-- 道具抚摸次数 -->
        <itemTouch>
            <Type> UINT32 </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </itemTouch>

        <itemList>
            <Type> ARRAY <of> Baby_ItemInfo </of> </Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </itemList>

        <GetRewardList><!--好友申请列表-->
            <Type>			ARRAY <of> INT32	 </of> 		</Type>
			<Flags>			BASE				</Flags>
            <Persistent> true </Persistent>
        </GetRewardList>

    </Properties>

</root>
