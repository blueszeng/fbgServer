        <keepExp>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </keepExp>
        <!-- 意识守门加成 -->
        <keepM>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </keepM>
        <!-- 技术 -->
        <tech>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
