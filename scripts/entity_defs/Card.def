        <defend>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </defend>
         <!-- 防守经验 -->
        <defendExp>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </defendExp>
        <!-- 意识防守加成 -->
        <defendM>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </defendM>
        <!-- 传球 -->
        <passBall>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </passBall>
        <!-- 传球经验 -->
        <passBallExp>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </passBallExp>
        <!-- 意识传球加成 -->
        <passBallM>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </passBallM>
        <!-- 拦截 -->
        <trick>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </trick>
