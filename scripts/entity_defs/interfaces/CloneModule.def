<root>
	<Implements>
	</Implements>

	<Properties>

        <passCloneInfo>

            <Type>			ARRAY <of> CloneInfo	 </of> 		</Type>
			<Flags>			BASE				</Flags>
            <Persistent> true </Persistent>
        </passCloneInfo>



        <cloneID>
			<Type> UINT32 </Type>
			<Flags> CELL_PRIVATE </Flags>
		</cloneID>

	</Properties>

	<BaseMethods>
        <onClientReqEnterClone>
            <Exposed/>
           <Arg> UINT32 </Arg>
        </onClientReqEnterClone>

        <!--离开副本-->
        <onClientLeaveClone>
            <Exposed/>
        </onClientLeaveClone>


	</BaseMethods>

	<CellMethods>
         <!--客户端调用 告诉服务器开始战斗-->
        <onClientBeginFight>
            <Exposed/>
        </onClientBeginFight>

       <!--客户端调用 告诉服务器动画播放完毕-->
        <onClientPlayAnimFinish>
             <Exposed/>
        </onClientPlayAnimFinish>

        <!--客户端调用 选择传球还是射门-->
        <onClientSelectOp>
            <Exposed/>
             <Arg> INT8 </Arg>
             <Arg>ARRAY <of> UINT32 </of></Arg>
             <Arg>ARRAY <of> UINT32 </of></Arg>
        </onClientSelectOp>

        <onCloneGM>
            <Exposed/>
            <Arg> UINT32 </Arg>
        </onCloneGM>
        <onGmSetSkill>
            <Exposed/>
            <Arg> UINT32 </Arg>
        </onGmSetSkill>

	</CellMethods>

   <ClientMethods>
       <!--通知客户端进攻者和防守者的id-->
        <onAtkAndDefID>
              <Arg>UINT32 </Arg>
             <Arg>ARRAY <of> UINT64 </of></Arg>
             <Arg>ARRAY <of> UINT32</of></Arg>
             <Arg>ARRAY <of> UINT64 </of></Arg>
             <Arg>ARRAY <of> UINT64 </of></Arg>
             <Arg>ARRAY <of> UINT64 </of></Arg>
             <Arg> UINT32 </Arg>
        </onAtkAndDefID>


        <!--通知客户端准备好了-->
        <onReady>
        </onReady>

         <!--通知当前回合-->
        <onCurAttackIndex>
             <Arg>UINT8</Arg>
        </onCurAttackIndex>

       <onOprateResult>
            <Arg>UINT8</Arg>
            <Arg>UINT8</Arg>
       </onOprateResult>

       <onSelectSkill>
            <Arg>UINT8</Arg>
            <Arg>ARRAY <of> UINT32 </of></Arg>
       </onSelectSkill>

       <!--通知客户端本轮结束-->
       <onRoundEnd>
           <Arg>UINT8</Arg> <!--我的得分-->
           <Arg>UINT8</Arg> <!--对方得分-->
       </onRoundEnd>
        <!--比赛结束-->
       <onGameOver>
       </onGameOver>

       <onTotalAttackTimes>
           <Arg>UINT8</Arg>
       </onTotalAttackTimes>

        <onSkillSucc>
            <!--技能使用成功的卡牌ID-->
            <Arg> ARRAY <of> UINT32 </of></Arg>
        </onSkillSucc>

       <onSkillError>
           <Arg>UINT8</Arg>
       </onSkillError>


      <onMyCardIdList>
          <!--所有卡牌的id-->
           <Arg>ARRAY <of> UINT32 </of></Arg>
       </onMyCardIdList>


	</ClientMethods>
</root>

