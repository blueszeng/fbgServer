<root>
    <Implements>
        <Interface>	CardEquipModule	 </Interface>
	</Implements>
    <Properties>
        <roleID>
            <Type>DBID</Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </roleID>
        <!-- 是否是本身所代表的的卡牌 -->
        <isSelf>
            <Type>UINT8</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </isSelf>

        <!-- 配置ID -->
        <configID>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </configID>

        <!--突破花费碎片数量-->
        <strikeNeedCost>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </strikeNeedCost>

        <!--等级-->
        <level>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
            <Default>1</Default>
        </level>
        <!--经验-->
        <exp>
            <Type>UINT64</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </exp>
        <!--星级-->
        <star>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </star>

        <!--突破层数-->
        <brokenLayer>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
        </brokenLayer>

         <!--战斗力-->
        <fightValue>
            <Type>UINT32</Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
            <Default>0</Default>
