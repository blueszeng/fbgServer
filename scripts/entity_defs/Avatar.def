<root>
	<Implements>
        <Interface>	MailsModule		    </Interface><!--邮件系统-->
        <Interface>	LotteryModule		    </Interface><!--抽卡系统-->
        <Interface>	ShopModule		        </Interface><!--商城系统-->
        <Interface>	FriendModule		    </Interface><!--好友系统-->
        <Interface>	BodyPowerModule		</Interface><!--体力系统-->
        <Interface>	ChatModule		        </Interface><!--聊天系统-->
        <Interface>	BagModule		        </Interface><!--背包系统-->
        <Interface>	CardMgrModule		    </Interface><!--卡牌管理系统-->
        <Interface>	PiecesModule		    </Interface>
        <Interface>	SlevelModule		    </Interface>
        <Interface>	EquipModule		    </Interface><!--装备系统-->
        <Interface>	CloneModule		    </Interface><!--副本系统-->
        <Interface>	MentalityModule		</Interface><!--意识系统-->
        <Interface>	StrikeModule		    </Interface><!--强化系统-->
        <Interface>	InheritModule		    </Interface><!--继承系统-->
        <Interface>	AbilityModule		    </Interface><!--能力系统-->
        <Interface>	BabyModule		    	</Interface><!--足球宝贝系统-->
        <Interface>	OfficialModule		    </Interface><!--官职系统-->
        <Interface>	GuildModule		    </Interface><!--公会系统-->
        <Interface>    FormationModule       </Interface><!--阵型系统-->
        <Interface>    GameShopModule        </Interface><!--游戏商城-->
        <Interface>    ArenaModule           </Interface><!--竞技场-->
        <Interface>    RankModule            </Interface><!--排行榜-->
        <Interface>    SkillModule            </Interface><!--技能-->
	</Implements>

	<Properties>

		<!--  主属性(需要存DB)begin -->
		<!-- 玩家昵称 -->
		<name>
			<Type> UNICODE </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
		</name>
        <!--账号ID-->
        <roleId>
            <Type> UINT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
        </roleId>
        <!--账号头像-->
        <photoIndex>
            <Type> STRING </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
        </photoIndex>
        <!--等级-->
		<level>
			<Type>	UINT16					</Type>
			<Flags>	BASE_AND_CLIENT		</Flags>
			<Persistent>	true				</Persistent>
            <Default> 1 </Default>
		</level>
        <!--经验-->
        <exp>
			<Type>	UINT32					</Type>
			<Flags>	BASE_AND_CLIENT	    </Flags>
			<Persistent>	true			</Persistent>
            <Default> 0 </Default>
		</exp>
        <!--个性签名-->
        <slogan>
            <Type> UNICODE </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
        </slogan>
        <club>
            <Type> UNICODE </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
        </club>

        <!--战斗力-->
        <fightValue>
            <Type> UINT64 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
        </fightValue>
        <!--vip等级-->
        <vipLevel>
            <Type>	UINT8						</Type>
			<Flags>	BASE_AND_CLIENT						</Flags>
			<Persistent>	true						</Persistent>
            <Default> 1 </Default>
        </vipLevel>
        <!--球员位置-->
        <job>
			<Type>	UINT8						</Type>
			<Flags>	BASE_AND_CLIENT						</Flags>
			<Persistent>	true						</Persistent>
		</job>


        <!--欧元（游戏中送）-->
		<euro>
			<Type> UINT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
			<Default> 0 </Default>
		</euro>

		<!--玩家钻石-->
		<diamond>
			<Type> UINT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
			<Default> 0 </Default>
		</diamond>

        <!--玩家充值RMB（主要用来计算vip,现实人民币）-->
		<rmb>
			<Type> UINT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
			<Default> 0 </Default>
		</rmb>

        <lastTime>
            <Type> STRING </Type>
			<Flags> BASE </Flags>
			<Persistent> true </Persistent>
        </lastTime>

        <logoutTime>
            <Type> UINT64 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
        </logoutTime>


        <formation>
            <Type> INT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
        </formation>

        <!-- 阵营 -->
        <camp>
            <Type> INT8 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
        </camp>
	</Properties>

	<BaseMethods>
        <onPlayerMgrCmd>
             <Arg> STRING </Arg>
             <Arg> PY_DICT </Arg>
        </onPlayerMgrCmd>


    <onClientGM>
            <Exposed/>
            <Arg> STRING </Arg>
    </onClientGM>


    <onClientGetPlayerInfo>
            <Exposed/>
            <Arg> UINT32 </Arg>
    </onClientGetPlayerInfo>


    <onClientChangeSolgan>
            <Exposed/>
            <Arg> UNICODE </Arg>
    </onClientChangeSolgan>

    <onClientGmAddAll>
        <Exposed/>
    </onClientGmAddAll>

    <onRoomEndResult>
         <Arg> UINT32 </Arg>
         <Arg> UINT32 </Arg>
         <Arg> UINT32 </Arg>
         <Arg> UINT32 </Arg>
    </onRoomEndResult>





	</BaseMethods>

	<CellMethods>

	</CellMethods>

	<ClientMethods>
        <onEnterScene><!--通知客户端进入场景-->
        </onEnterScene>


        <onOperateSuc>
            <Arg> STRING </Arg>  <!--方法名-->
        </onOperateSuc>

	    <onGiftError>
	         <Arg>UINT32</Arg>
	    </onGiftError>
        <onCardFightValueChange>
            <Arg>UINT64</Arg>
            <Arg>UINT64</Arg>
        </onCardFightValueChange>

        <onGetPlayerInfo>
            <Arg> PlayerInfo </Arg>
        </onGetPlayerInfo>

	</ClientMethods>




</root>

