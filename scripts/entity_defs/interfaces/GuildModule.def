<root>
	<Implements>
	</Implements>

	<Properties>
        <guildDBID>
			<Type> UINT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
            <Persistent> true </Persistent>
		</guildDBID>

        <!--个人当前贡献点-->
        <guildDonate>
			<Type> UINT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
            <Persistent> true </Persistent>
		</guildDonate>

        <!--公会职位-->
        <guildPower>
			<Type> UINT32 </Type>
			<Flags> BASE_AND_CLIENT </Flags>
            <Persistent> true </Persistent>
		</guildPower>

        <!--公会商城等级-->
        <guildShopLevel>
			<Type> UINT32 </Type>
			<Flags> BASE </Flags>
            <Default>1</Default>
            <Persistent> true </Persistent>
		</guildShopLevel>

       <applyGuildList>
			<Type> ARRAY <of> UINT64 </of> </Type>
			<Flags> BASE </Flags>
            <Persistent> true </Persistent>
		</applyGuildList>

        <!--公会名字-->
        <guildName>
            <Type> UNICODE </Type>
			<Flags> BASE_AND_CLIENT </Flags>
			<Persistent> true </Persistent>
       </guildName>


	</Properties>

	<BaseMethods>
            <!--客户端方法-->
        <onClientCreateGuild>
            <Exposed/>
            <Arg> UNICODE </Arg> <!-- 公会名字 -->
            <Arg> UNICODE </Arg> <!-- 公会介绍 -->
        </onClientCreateGuild>
        <!--获得公会信息-->
        <onClientGetGuildInfo>
            <Exposed/>
        </onClientGetGuildInfo>

        <!--请求公会列表-->
        <onClientGuildList>
            <Exposed/>
        </onClientGuildList>

        <!--请求公会成员列表-->
        <onClientGetGuildMember>
            <Exposed/>
        </onClientGetGuildMember>

        <!--请求公会申请加入列表-->
        <onClientGetGuildApply>
            <Exposed/>
        </onClientGetGuildApply>

        <!--查找派系-->
        <onClientFindCamp>
            <Exposed/>
            <Arg> UNICODE </Arg> <!-- 公会关键字 -->
        </onClientFindCamp>

        <!--申请加入公会-->
        <onClientApplyJoinGuild>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 公会DBID-->
        </onClientApplyJoinGuild>

        <!--公会弹劾-->
        <onClientImpeach>
            <Exposed/>
        </onClientImpeach>


        <!--公会开除-->
        <onClientKickOut>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 成员ID-->
        </onClientKickOut>

        <!--建筑升级-->
        <onClientBuildUpgrade>
            <Exposed/>
            <Arg> UINT32 </Arg> <!-- 建筑ID-->
        </onClientBuildUpgrade>

        <!--建筑升级加速-->
        <onClientBuildSpeed>
            <Exposed/>
            <Arg> UINT32 </Arg> <!-- 建筑ID-->
            <Arg> UINT32 </Arg> <!-- 加速小时-->
        </onClientBuildSpeed>

         <!--GM增加公会资金-->
        <onClientGMAddGuildFunds>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 资金-->
        </onClientGMAddGuildFunds>

        <!--GM增加公会声望-->
        <onClientGMAddGuildReputation>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 资金-->
        </onClientGMAddGuildReputation>

        <!--拒绝申请-->
        <onClientRejectApply>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 玩家DBID-->
        </onClientRejectApply>

        <!--接受玩家加入申请-->
        <onClientAgreeJoin>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 玩家DBID-->
        </onClientAgreeJoin>

        <!--取消申请-->
        <onClientCancelApply>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 公会DBID-->
        </onClientCancelApply>

        <!--公会上诉曝光-->
        <onClientAppealExposure>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 公会DBID-->
            <Arg> UINT32 </Arg> <!-- 上诉曝光ID-->
        </onClientAppealExposure>

         <!--公会购买保护时间-->
        <onClientBuyGuildProtect>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 保护时间-->
        </onClientBuyGuildProtect>

        <!--申请公会副队及简介-->
        <onClientGetViceIntroduce>
            <Exposed/>
            <Arg> UINT64 </Arg> <!-- 公会DBID-->
        </onClientGetViceIntroduce>

        <!--修改公告和简介-->
        <onClientChangeNotice>
            <Exposed/>
            <Arg> UINT8 </Arg> <!-- 公会简介是否修改 1 修改-->
            <Arg> UNICODE </Arg> <!-- 简介 -->
            <Arg> UINT8 </Arg> <!-- 公告是否修改 1 修改-->
            <Arg> UNICODE </Arg> <!-- 公告 -->
        </onClientChangeNotice>


        <!--修改公会名字-->
        <onClientChangeName>
            <Exposed/>
            <Arg> UNICODE </Arg> <!-- 公会新名字-->
        </onClientChangeName>

             <!--公会任命-->
        <onClientAppointPower>
            <Exposed/>
            <Arg> UINT32 </Arg> <!-- 公会DBID-->
            <Arg> UINT32 </Arg> <!-- 公会DBID-->
        </onClientAppointPower>

        <!--解散公会-->
        <onClientDismissGuild>
            <Exposed/>
        </onClientDismissGuild>

        <!--取消解散公会-->
        <onClientCancelDismiss>
            <Exposed/>
        </onClientCancelDismiss>

        <!--退出公会-->
        <onClientQuitGuild>
            <Exposed/>
        </onClientQuitGuild>

        <!--公会捐赠-->
        <onClientDonate>
            <Exposed/>
            <Arg> UINT32 </Arg> <!-- 次数-->
        </onClientDonate>


        <!--公会转让-->
        <onClientGuildTransfer>
            <Exposed/>
            <Arg> UINT32 </Arg> <!-- 成员ID-->
        </onClientGuildTransfer>


        <setGuildDBID>
            <Arg> PY_DICT </Arg>
        </setGuildDBID>

        <setApplyGuildDBIDList>
            <Arg> PY_DICT </Arg>
        </setApplyGuildDBIDList>

	</BaseMethods>

	<CellMethods>

	</CellMethods>

   <ClientMethods>
        <!--查找自己的派系-->
        <onFindCamp>

           <Arg> UINT32 </Arg>        <!-- 数量-->
           <Arg> ARRAY <of> GuildMgr_ItemInfo </of></Arg>
       </onFindCamp>


       <!--获取公会副会长及简介-->
       <onGuildViceIntroduce>
           <Arg> ARRAY <of> Guild_MemberInfo </of></Arg>
           <Arg> UNICODE </Arg>        <!-- 公会简介-->
       </onGuildViceIntroduce>

        <!--公会申请人列表-->
       <onGuildApplyList>
           <Arg> ARRAY <of> Guild_ApplyMemberInfo </of></Arg>
       </onGuildApplyList>


       <!--公会成员列表-->
       <onGuildMemberList>
           <Arg> ARRAY <of> Guild_MemberInfo </of></Arg>
       </onGuildMemberList>


       <!--已经申请过的公会列表-->
       <onApplyGuildIDList>
           <Arg> ARRAY <of> UINT64 </of></Arg>
       </onApplyGuildIDList>

       <!--公会信息-->
        <onGetGuildInfo>
           <Arg> UINT32 </Arg>          <!-- 公会等级-->
           <Arg> UNICODE </Arg>        <!-- 公会名字-->
           <Arg> UINT32 </Arg>          <!-- 公会人数-->
           <Arg> UINT32 </Arg>         <!-- 公会基金-->
           <Arg> UINT32 </Arg>        <!-- 公会声望-->
           <Arg> UNICODE </Arg>        <!-- 公会公告-->
            <Arg> UINT64 </Arg>        <!-- 公会解散时间-->
            <Arg> UINT64 </Arg>        <!-- 公会ID-->
            <Arg> UNICODE </Arg>        <!-- 公会简介-->
            <Arg> UINT64 </Arg>        <!-- 保护时间-->
            <Arg> ARRAY <of> Guild_Build </of></Arg> <!-- 建筑-->

       </onGetGuildInfo>


       <!--返回公会建筑信息-->
       <onClientGuildBuildInfo>
           <Arg> Guild_Build </Arg>        <!-- 建筑-->
       </onClientGuildBuildInfo>


       <onGuildError>
           <Arg>UINT8</Arg>
       </onGuildError>

      <onResponse>
           <Arg>UINT8</Arg>
       </onResponse>

       <!--公会升级成功-->
       <onGuildUpgradeSucc>
           <Arg>UINT8</Arg> <!-- 公会等级-->
           <Arg>UINT32</Arg><!-- 公会声望-->
       </onGuildUpgradeSucc>

       <!--公会捐献成功-->
       <onGuildDonateSucc>
           <Arg>UINT32</Arg><!-- 公会资金-->
           <Arg> Guild_MemberInfo </Arg>
       </onGuildDonateSucc>

          <!--公会刷新成员信息-->
       <onGuilRefreshMemeber>
           <Arg> Guild_MemberInfo </Arg>
       </onGuilRefreshMemeber>

           <!--公会返回修改公告-->
       <onChangeNoticeSucc>
           <Arg> UNICODE </Arg>        <!-- 公会公告-->
           <Arg> UNICODE </Arg>        <!-- 公会简介-->

       </onChangeNoticeSucc>

           <!--公会返回修改名字-->
       <onChangeNameSucc>
           <Arg> UNICODE </Arg>        <!-- 公会名字-->
       </onChangeNameSucc>

      <!--公会刷新公会资金-->
       <onClientGuildFunds>
           <Arg> UINT64 </Arg>        <!-- 公会资金-->
       </onClientGuildFunds>

       <!--公会准备解散-->
       <onGuildReadyDismiss>
           <Arg> UINT64 </Arg>        <!-- 公会名字-->
       </onGuildReadyDismiss>

      <!--公会开除成功-->
       <onKictOutSucc>
            <Arg> UINT32 </Arg> <!-- 次数-->
       </onKictOutSucc>

       <!--购买公会保护成功-->
       <onBuyGuildProtectSucc>
           <Arg> UINT64 </Arg>        <!-- 公会名字-->
       </onBuyGuildProtectSucc>

	</ClientMethods>
</root>

