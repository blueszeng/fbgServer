        <trickExp>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
        </trickExp>
        <!-- 意识拦截加成 -->
        <trickM>
            <Type>UINT32</Type>
            <Flags>BASE_AND_CLIENT</Flags>
            <Persistent>true</Persistent>
