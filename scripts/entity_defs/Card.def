        <equips>
            <Type>ARRAY <of> Equip_Info</of>></Type>
            <Flags>BASE</Flags>
            <Persistent>true</Persistent>
        </equips>

        <baseProp>
            <Type>PYTHON</Type>
            <Flags>CELL_PUBLIC</Flags>
        </baseProp>
        <!--球场上的坐标-->
        <coordinate>
            <Type>UINT32</Type>
            <Flags>ALL_CLIENTS</Flags>
        </coordinate>

        <!--配置ID-->
        <configID_B>
            <Type>UINT32</Type>
            <Flags>ALL_CLIENTS</Flags>
        </configID_B>




    </Properties>

    <BaseMethods>
    </BaseMethods>



	<CellMethods>
        <!--传送数据到cell-->
        <onSetBaseProp>
            <Arg> PYTHON </Arg>  <!--方法名-->
        </onSetBaseProp>
	</CellMethods>
</root>
